import time

def find_neighbours(word,word_list):
    list_of_neighbours = []
    for sample_word in word_list:
        if len(sample_word) != len(word):
            pass
        else:
            count = 0
            i = 0
            for i in range(len(word)):
                if sample_word[i] != word[i]:
                    count = count + 1
            if count == 1:
                list_of_neighbours.append(sample_word)
    return list_of_neighbours
                
